{
  "name": "gabi-yoga-lambda-infrastructure",
  "version": "1.0.0",
  "description": "CDK Infrastructure for Gabi Yoga Lambda Migration",
  "bin": {
    "lambda-infrastructure": "bin/lambda-stack.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap --profile gabi",
    "deploy:dev": "cdk deploy --all --profile gabi --require-approval never --context stage=dev",
    "deploy:prod": "cdk deploy --all --profile gabi --require-approval never --context stage=prod",
    "destroy:dev": "cdk destroy --all --profile gabi --force --context stage=dev",
    "destroy:prod": "cdk destroy --all --profile gabi --force --context stage=prod",
    "diff:dev": "cdk diff --all --context stage=dev",
    "diff:prod": "cdk diff --all --context stage=prod",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "aws-cdk": "2.100.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "2.100.0",
    "constructs": "^10.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "lambda",
    "dynamodb",
    "api-gateway",
    "serverless",
    "yoga",
    "webapp"
  ],
  "author": "Gabi Yoga Team",
  "license": "MIT"
}