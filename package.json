{
  "name": "yoga-website",
  "version": "1.0.0",
  "description": "Gabi Yoga Website with Database",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "seed": "node database/seed.js",
    "init-db": "node -e \"require('./database/schema').initializeDatabase()\"",
    "reset-db": "NODE_ENV=development node database/seed.js",
    "setup-images": "node setup-image-storage.js",
    "migrate-images": "node migrate-images-cli.js",
    "add-file-path-column": "node database/add-file-path-column.js",
    "export-templates": "node database/seed-templates.js --export-only",
    "import-templates": "node database/seed-templates.js --import-only",
    "sync-templates": "node database/seed-templates.js",
    "deploy": "cd infrastructure && cdk deploy --all",
    "cdk-bootstrap": "cd infrastructure && cdk bootstrap",
    "cdk-synth": "cd infrastructure && cdk synth"
  },
  "dependencies": {
    "aws-sdk": "^2.1442.0",
    "bcryptjs": "^2.4.3",
    "browser-image-compression": "^2.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.6.0",
    "puppeteer": "^24.6.1",
    "sqlite3": "^5.1.6",
    "stripe": "^12.18.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "aws-cdk": "^2.100.0",
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.2.69",
    "nodemon": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
